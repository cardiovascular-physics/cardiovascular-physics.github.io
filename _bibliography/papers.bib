@article{lazarevic_efficacy_2011,
	title = {Efficacy and safety of short-term genistein intervention in patients with localized prostate cancer prior to radical prostatectomy: a randomized,  placebo-controlled, double-blind {Phase} 2 clinical trial.},
	volume = {63},
	issn = {1532-7914 0163-5581},
	doi = {10.1080/01635581.2011.582221},
	abstract = {We conducted a placebo-controlled, block-randomized double-blind Phase 2 study to examine the effect of 30 mg synthetic genistein daily on serum and tissue  biomarkers in patients with localized prostate cancer (CaP). Fifty-four study  subjects were recruited and randomized to treatment with genistein (n = 23) or  placebo (n = 24) for 3 to 6 wk prior to prostatectomy. Seven study subjects were  noncompliant to the study protocol. Adverse events were few and mild. Serum  prostate specific antigen (PSA) decreased by 7.8\% in the genistein arm and  increased by 4.4\% in the placebo arm (P = 0.051). The PSA level was reduced in  tumor tissue compared to normal tissue in the placebo arm. In the genistein arm,  the PSA level in tumor and normal tissue was comparable. Total cholesterol was  significantly lower in the genistein arm (P = 0.013). There were no significant  effects on thyroid or sex hormones. Plasma concentrations of total genistein were  on average 100-fold higher in the genistein arm after treatment (P {\textless} 0.001).  Genistein at a dose that can be easily obtained from a diet rich in soy reduced  the level of serum PSA in patients with localized CaP, without any effects on  hormones. It was well tolerated and had a beneficial effect on blood cholesterol.},
	language = {eng},
	number = {6},
	journal = {Nutrition and cancer},
	author = {Lazarevic, Bato and Boezelijn, Gro and Diep, Lien My and Kvernrod, Kristin and Ogren, Olov and Ramberg, Hakon and Moen, Anders and Wessel, Nicolai and Berg, R. Egil and Egge-Jacobsen, Wolfgang and Hammarstrom, Clara and Svindland, Aud and Kucuk, Omer and Saatcioglu, Fahri and Taskèn, Kristin A. and Karlsen, Steinar J.},
	year = {2011},
	pmid = {21714686},
