	pmcid = {PMC4031946},
	note = {Place: United States},
	keywords = {*Dietary Supplements, Adolescent, Adult, Cytochrome P-450 Enzyme System/*metabolism, cytochrome P450, Eicosanoic Acids/*blood, Fatty Acids, Omega-3/*administration \& dosage, Female, Humans, lipidomics, Male, Middle Aged, nutrition},
	pages = {1150--1164},
}

