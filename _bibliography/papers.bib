	author = {Wessel, N. and Malberg, H. and Heringer-Walther, S. and Schultheiss, H.-P. and Walther, T.},
	month = feb,
	year = {2007},
	pmid = {17312445},
	note = {Place: United States},
	keywords = {Angiotensin I/*metabolism, Animals, Baroreflex/*drug effects, Blood Pressure/drug effects, Circadian Rhythm/*drug effects, Heart Rate/drug effects, Imidazoles/*pharmacology, Male, Peptide Fragments/*metabolism, Rats, Rats, Inbred SHR, Receptors, Angiotensin/*agonists, Renin-Angiotensin System/drug effects},
	pages = {67--73},
}

