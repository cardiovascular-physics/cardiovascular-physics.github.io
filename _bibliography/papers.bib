	note = {Place: United States},
	keywords = {*Models, Cardiovascular, *Nonlinear Dynamics, Algorithms, Animals, Cardiovascular Diseases/*diagnosis/*therapy, Computer Simulation, Diagnosis, Computer-Assisted/*methods, Heart/*physiopathology, Humans, Therapy, Computer-Assisted/*methods},
	pages = {015101},
