	pmid = {17411273},
	note = {Place: United States},
	keywords = {*Heart Rate, *Models, Biological, *Pulmonary Ventilation, *Respiratory Mechanics, *Sleep, Animals, Biological Clocks, Diagnosis, Computer-Assisted/methods, Humans, Oscillometry/methods, Polysomnography/*methods, Sleep Wake Disorders/diagnosis/*physiopathology},
