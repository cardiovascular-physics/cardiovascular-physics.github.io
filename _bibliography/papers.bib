@article{reyes-lagos_symbolic_2019,
	title = {Symbolic analysis of heart rate fluctuations identifies cardiac autonomic modifications during {LPS}-induced endotoxemia.},
	volume = {221},
	copyright = {Copyright © 2019 Elsevier B.V. All rights reserved.},
	issn = {1872-7484 1566-0702},
	doi = {10.1016/j.autneu.2019.102577},
	abstract = {The present study aimed to compare linear and symbolic dynamics (SD) indices for detecting the autonomic cardiac changes produced by endotoxemia in freely-moving  rats. In this context, we analyzed ECG-derived R-R time series in freely moving  Dark Agouti rats, which received lipopolysaccharide (LPS, n = 9), or vehicle (V,  n = 7). Five minutes R-R time series were assessed every hour up to +12 h  and + 24 h post-LPS injection. We found that SD indices showed significant  differences at +7 h between V vs. LPS groups and at +9 h between basal levels of  LPS (-3 h) and post-LPS injection (pre-LPS vs. post-LPS). In general, SD seems  more appropriate than linear indices to evaluate the autonomic changes of  endotoxemic rats. Overall, the symbolic parameters detected decreased R-R  variability and complexity, which indicate a modification of the autonomic  regulation during LPS-induced endotoxemia. This modification is probably related  to a reduced activity of the cholinergic anti-inflammatory pathway at the long  term.},
	language = {eng},
	journal = {Autonomic neuroscience : basic \& clinical},
	author = {Reyes-Lagos, José Javier and Ledesma-Ramírez, Claudia Ivette and Hadamitzky, Martin and Peña-Castillo, Miguel Ángel and Echeverría, Juan C. and Lückemann, Laura and Schedlowski, Manfred and Berg, Karsten and Wessel, Niels and Pacheco-López, Gustavo},
	month = nov,
	year = {2019},
	pmid = {31445407},
	note = {Place: Netherlands},
	keywords = {Animals, Autonomic function, Endotoxemia, Endotoxemia/chemically induced/*physiopathology, Heart Conduction System/*physiopathology, Heart Rate/*physiology, HRV, Lipopolysaccharides/toxicity, LPS, Male, Nonlinear Dynamics, Rats, Rats, Inbred Strains, Symbolic dynamics, Vagus Nerve/*physiopathology},
	pages = {102577},
}

